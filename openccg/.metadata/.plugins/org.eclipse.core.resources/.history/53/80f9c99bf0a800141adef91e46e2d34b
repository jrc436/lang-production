package ar.xmlparser;

import java.util.Scanner;

public class LineParser {
	public void parse(String line) throws Exception {
		line.trim();
		Scanner s = new Scanner(line);
		String iter = "start";
		while (s.hasNext()) {
			String cur = s.next();
			switch(iter) {
				case "start":
					if (cur.charAt(0) != '<') {
						throw new Exception("Format error. Line without leading '<'");
					}
				default:
			}
		}
		
		
		s.close();
	}
}
